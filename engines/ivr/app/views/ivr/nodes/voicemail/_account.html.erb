<tr id="node_a_<%= num %>">
  <td width="75%"><%= lib f, :accounts, "[related_accounts][a_#{num}][libs_accounts_id]", libs_accounts_id %></td>

  <td width="25%" class="block">
    <% if num == 0 %>
      <%= link_to icon('plus-square', nil, class: 'fa-1-5x'), "javascript:new_voicemail_node_account('#{escape_javascript(render 'ivr/nodes/voicemail/account', f: f, num: 'x', libs_accounts_id: libs_accounts_id)}');" %>
    <% else %>
      <%= link_to icon('minus-square', nil, class: 'fa-1-5x'), "javascript:remove_object($('#node_a_#{num}'));" %>
    <% end %>
  </td>
</tr>